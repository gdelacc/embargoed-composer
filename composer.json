{
    "name": "gdelacc/embargoed-composer",
    "description": "Embargoed Composer is a simple PHP Composer library to blocks all requests from Russia and displays a pro-Ukraine message instead. Based on https://github.com/rameerez/embargoed",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo d.",
            "email": "gdelacruz@prontonmail.ch"
        }
    ],
    "autoload": {
        "psr-4": {"embargoedComposer\\": "src/"},
        "files": [
            "embargoed-composer.php"
        ]
    },
    "require": {
        "maxmind-db/reader": "~1.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/gdelacc/embargoed-composer"
        }
    ],
    "minimum-stability": "dev"
}
